{
  "name": "shoetrak-monorepo",
  "description": "Mono repositÃ³rio para aplicativos e pacotes da Shoetrak.",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.5.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "dotenv -- turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prisma:deploy": "dotenv -- turbo prm:mig:dpl"
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "dotenv-cli": "^7.4.1",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "turbo": "latest"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "next-auth": "^4.24.7"
  }
}
